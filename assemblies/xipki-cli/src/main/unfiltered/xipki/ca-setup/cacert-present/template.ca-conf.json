{
	"basedir":"APP_DIR",
	"properties":{
		"profiles.dir":"xipki/ca-setup/certprofile",
		"token.type":"REPLACEME-TOKEN_TYPE",
		"ca.signerConf":"REPLACEME-CA_SIGNERCONF"
	},
	"signers":[{
		"name":"responder1",
		"type":"pkcs12",
		"conf":{
			"value":"password=1234,keystore=file:xipki/keycerts/tlskeys/server/tls-server.p12,algo=SHA256withRSA"
		}
	},{
		"name":"scep-responder1",
		"type":"pkcs12",
		"conf":{
			"value":"password=1234,keystore=file:xipki/ca-setup/keycerts/scep1.p12,algo=SHA256withRSA"
		}
	}],
	"requestors":[{
		"name":"requestor1",
		"type":"cert",
		"binaryConf":{
			"file":"xipki/keycerts/tlskeys/client/tls-client-cert.der"
		}
	},{
		"name":"requestor-mac1",
		"type":"pbm",
		"conf":{
			"value":"123456"
		}
	}],
	"users":[{
		"name":"user1",
		"active":true,
		"password":"password1"
	}],
	"publishers":[{
		"name":"ocsp-publisher",
		"type":"ocsp",
		"conf":{
			"value":"datasource=ocsp,publish.goodcerts=true"
		}
	}],
	"profiles":[{
		"name":"scep",
		"type":"xijson",
		"conf":{
			"file":"${profiles.dir}/certprofile-scep.json"
		}
	},{
		"name":"ocsp",
		"type":"xijson",
		"conf":{
			"file":"${profiles.dir}/certprofile-ocsp.json"
		}
	},{
		"name":"tls",
		"type":"xijson",
		"conf":{
			"file":"${profiles.dir}/certprofile-tls.json"
		}
	},{
		"name":"tls-c",
		"type":"xijson",
		"conf":{
			"file":"${profiles.dir}/certprofile-tls-c.json"
		}
	}],
	"cas":[{
		"name":"myca1",
		"aliases":[
			"myca"
		],
		"caInfo":{
			"caUris":{
				"crlUris":[
					"http://ca.example.org/crl"
				],
				"ocspUris":[
					"http://localhost:8080/ocsp/responder1"
				]
			},
			"cmpControl":{
				"send.ca":true,
				"messagetime.bias":300,
				"rr.aki.required":true,
				"group.enroll":true,
				"protection.sigalgo":"SHA256withRSA:SHA256withECDSA",
				"popo.sigalgo":"SHA256withRSA:SHA256withECDSA:SHA256withDSA:SM3withSM2"
			},
			"cmpResponderName":"responder1",
			"crlControl":{
				"update.mode":"interval",
				"fullcrl.intervals":1,
				"overlap.minutes":90,
				"interval.time":"02:00"
			},
			"duplicateKey":true,
			"duplicateSubject":true,
			"expirationPeriod":365,
			"cert":{
				"file":"xipki/ca-setup/keycerts/myca1.der"
			},
			"keepExpiredCertDays":-1,
			"maxValidity":"10y",
			"nextCrlNo":2,
			"permissions":["all"],
			"protocolSupport":["cmp","rest","scep"],
			"saveReq":false,
			"scepControl":{
				"cacert.included":true,
				"signercert.included":true,
				"support.getcrl":true
			},
			"scepResponderName":"scep-responder1",
			"signerType":"${token.type}",
			"signerConf":{
				"value":"${ca.signerConf}"
			},
			"snSize":63,
			"status":"active",
			"validityMode":"cutoff"
		},
		"profiles":[
			"ocsp",
			"scep",
			"tls",
			"tls-c"
		],
		"publishers":[
			"ocsp-publisher"
		],
		"requestors":[{
			"ra":true,
			"requestorName":"requestor1",
			"permissions":[
				"all"
			],
			"profiles":[
				"all"
			]
		},{
			"ra":true,
			"requestorName":"requestor-mac1",
			"permissions":[
				"all"
			],
			"profiles":[
				"all"
			]
		}],
		"users":[{
			"userName":"user1",
			"permissions":[
				"all"
			],
			"profiles":[
				"all"
			]
		}]
	}]
}

