{
    "basedir":"APP_DIR",
	"properties":{
		"token.type":"REPLACEME-TOKEN_TYPE",
		"ca.signerConf": "REPLACEME-CA_SIGNERCONF"
	},
	"cas":[{
		"name":"subcawithcrl1",
		"aliases":[
			"subcawithcrl"
		],
		"caInfo":{
			"caUris":{
				"cacertUris":[
					"http://example.org/subcawithcrl1.der"
				],
				"crlUris":[
					"http://subcawithcrl.example.org/crl"
				],
				"delteCrlUris":[
					"http://subcawithcrl.example.org/deltacrl"
				],
				"ocspUris":[
					"http://localhost:8080/ocsp/responder2"
				]
			},
			"cmpControl":{
				"send.ca":true,
				"messagetime.bias":300,
				"rr.aki.required":true,
				"group.enroll":true,
				"protection.sigalgo":"REPLACEME-CMP_SIGALGO",
				"popo.sigalgo":"REPLACEME-SIGALGO:REPLACEME-HASHwithRSA:REPLACEME-HASHwithECDSA:REPLACEME-HASHwithDSA:SM3withSM2"
			},
			"crlControl":{
				"update.mode":"interval",
				"fullcrl.intervals":1,
				"overlap.minutes":90,
				"interval.time":"02:00",
				"xipki.certset":false
			},
			"cmpResponderName":"responder1",
			"duplicateKey":true,
			"duplicateSubject":true,
			"expirationPeriod":365,
			"keepExpiredCertDays":-1,
			"maxValidity":"8000y",
			"nextCrlNo":2,
			"permissions":["all"],
			"protocolSupport":["cmp","rest","scep"],
			"saveReq":true,
			"scepControl":{
				"cacert.included":true,
				"signercert.included":true,
				"support.getcrl":true
			},
			"signerType":"${token.type}",
			"signerConf":{
				"value":"${ca.signerConf}"
			},
			"snSize":63,
			"status":"active",
			"validityMode":"lax"
		},
		"profiles":[
			"ocsp",
			"scep",
			"multiple-ous",
			"multi-valued-rdn",
			"max-time",
			"tlsa"
		],
		"requestors":[{
			"ra":true,
			"requestorName":"requestor1",
			"permissions":[
				"all"
			],
			"profiles":[
				"all"
			]
		},{
			"ra":true,
			"requestorName":"requestor-mac1",
			"permissions":[
				"all"
			],
			"profiles":[
				"all"
			]
		}],
		"users":[{
			"userName":"user1",
			"permissions":[
				"all"
			],
			"profiles":[
				"all"
			]
		}]
	}]
}

